@import url(http://fonts.googleapis.com/earlyaccess/notosansjapanese.css);

/* styles in src/style directory are applied to the whole page */
html,
body
  width 100%
  height 100%
  overflow hidden
  margin 0
  padding 0
  font-weight 100
  font-size 1em
  font-family 'Noto Sans Japanese', sans-serif

.align-center
  text-align center
